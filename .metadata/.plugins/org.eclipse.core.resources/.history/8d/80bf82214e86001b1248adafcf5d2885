import java.util.Scanner;
public class Menu {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int op = 0;
		op = sc.nextInt();
		System.out.println("Digite uma opção: ");
		do {
			System.out.println("Cadastrar Cliente");
			System.out.println("Listar os Clientes");
		}while (op != 0);

	}

}